#!/usr/bin/bash

wget -nc 'https://raw.githubusercontent.com/moby/moby/master/profiles/seccomp/default.json'

sed "s/\"writev\"/\"writev\",\"clone\",\"ptrace\",\"personality\"/" default.json > modified.json

echo 'modified.json generated.'
